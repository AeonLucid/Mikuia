{
	"name": "[BETA] Counter-Strike: Global Offensive",
	"description": "Stuff related to CS:GO",
	"baseFile": "plugin.iced",
	"dependencies": [
		"steam"
	],
	"hidden": false,
	"handlers": {
		"csgo.stats": {
			"description": "Shows CS:GO profile stats.",
			"anonymous": true,
			"settings": {
				"format": {
					"name": "Output Format",
					"description": "Format of the command output.",
					"default": "{{rankName}} ({{wins}} wins)",
					"type": "format",
					"variables": {
						"rankId": "MM Rank ID",
						"rankName": "MM Rank Name",
						"wins": "MM Win Count"
					}
				}
			}
		}
	}
}